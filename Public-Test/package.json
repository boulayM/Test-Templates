{
  "name": "angular-front-api-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "check:encoding": "powershell -ExecutionPolicy Bypass -File scripts/check-encoding.ps1",
    "prestart": "npm run check:encoding && npm run check:dom-xss",
    "start": "ng serve",
    "prebuild": "npm run check:encoding && npm run check:dom-xss",
    "build": "ng build",
    "prewatch": "npm run check:encoding && npm run check:dom-xss",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "predev": "npm run check:encoding && npm run check:dom-xss",
    "prepreview": "npm run check:encoding && npm run check:dom-xss",
    "check:dom-xss": "node scripts/check-dom-xss.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test:unit": "ng test --watch=false --browsers=ChromeHeadless",
    "setup:e2e": "powershell -ExecutionPolicy Bypass -File scripts/setup-e2e.ps1",
    "pree2e": "set E2E_RUN_SEED=true&& npm run setup:e2e",
    "e2e": "set PW_REUSE_SERVER=true&& set E2E_API_URL=http://localhost:3001/api&& playwright test",
    "pree2e:strict": "set E2E_RUN_SEED=true&& npm run setup:e2e",
    "e2e:strict": "set PW_REUSE_SERVER=false&& set E2E_API_URL=http://localhost:3001/api&& playwright test",
    "pree2e:quick": "set E2E_RUN_SEED=false&& npm run setup:e2e",
    "e2e:quick": "set PW_REUSE_SERVER=true&& set E2E_API_URL=http://localhost:3001/api&& playwright test",
    "pree2e:ui": "set E2E_RUN_SEED=false&& npm run setup:e2e",
    "e2e:ui": "set PW_REUSE_SERVER=true&& set E2E_API_URL=http://localhost:3001/api&& playwright test --ui",
    "pree2e:debug": "set E2E_RUN_SEED=false&& npm run setup:e2e",
    "e2e:debug": "set PW_REUSE_SERVER=true&& set E2E_API_URL=http://localhost:3001/api&& playwright test --debug",
    "build:stats": "ng build --stats-json",
    "seo:check": "node scripts/check-seo.mjs",
    "seo:perf:guide": "echo Run Lighthouse manually: npx lighthouse http://localhost:4200/home --preset=desktop --view",
    "e2e:seo": "playwright test -c playwright.seo.config.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.0",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/localize": "^21.1.4",
    "@angular/platform-browser": "^21.1.0",
    "@angular/platform-browser-dynamic": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@ng-bootstrap/ng-bootstrap": "^20.0.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^18.4.3",
    "@angular-eslint/eslint-plugin-template": "^18.4.3",
    "@angular-eslint/template-parser": "^18.4.3",
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@playwright/test": "^1.58.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "globals": "^15.15.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.7.4",
    "sass": "^1.97.0",
    "typescript": "~5.9.3"
  }
}
